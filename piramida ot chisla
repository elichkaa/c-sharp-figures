using System;


namespace BasicSyntaxCSharp
{
    class Program
    {
        static void Main(string[] args)
        {
            int endNum = int.Parse(Console.ReadLine());
            
            int currentNum = 1;

            for (int rows = 1; rows <= endNum; rows++)
            {
                for (int counter = 1; counter <= endNum; counter++)
                {
                    if (counter <= currentNum)
                    {
                        Console.Write($"{currentNum} ");
                    }
                    
                }
                Console.WriteLine();
                currentNum++;
            }
            
        }
    }
}
